{
  "name": "e2e-testing",
  "version": "0.1.0",
  "description": "End-to-End Testing with Nightwatch and Cucumber",
  "devDependencies": {},
  "scripts": {
    "e2e-debug": "node --inspect node_modules/nightwatch/bin/nightwatch",
    "nightwatch-cucumber-travis-ios": "node_modules/.bin/nightwatch -c ./nightwatch-cucumber-travis-ios.conf.js -e ios",
    "nightwatch-cucumber-ios": "node_modules/.bin/nightwatch -c ./nightwatch-cucumber-ios.conf.js -e default",
    "travis-ios": "node_modules/.bin/nightwatch -c ./nightwatch-ios.conf.js -e ios",
    "generate-report": "node generate-report.js"
  },
  "dependencies": {
    "appium-support": "latest",
    "appium-xciutest-driver": "latest",
    "appium-xcode": "latest",
    "appium-xcuitest-driver": "^2.80.0",
    "chromedriver": "2.37.0",
    "cucumber": "4.0.0",
    "cucumber-html-reporter": "4.0.2",
    "cucumber-pretty": "1.4.1",
    "electron": "1.8.6",
    "geckodriver": "1.11.0",
    "minimist": "1.2.0",
    "nightwatch": "0.9.20",
    "nightwatch-cucumber": "9.1.2",
    "selenium-server": "3.7.1",
    "selenium-standalone": "6.13.0"
  }
}
