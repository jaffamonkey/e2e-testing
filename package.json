{
  "name": "e2e-testing",
  "version": "0.1.0",
  "description": "End-to-End Testing with Nightwatch and Cucumber",
  "devDependencies": {},
  "scripts": {
    "e2e-debug": "node --inspect node_modules/nightwatch/bin/nightwatch",
    "selenium-install": "node_modules/.bin/selenium-standalone install",
    "selenium": "node_modules/.bin/selenium-standalone start",
    "nightwatch-cucumber-chrome": "node_modules/.bin/nightwatch -c ./nightwatch-cucumber.conf.js -e default",
    "nightwatch-cucumber-travis": "node_modules/.bin/nightwatch -c ./nightwatch-cucumber-travis.conf.js -e default",
    "nightwatch-cucumber-parallel": "node_modules/.bin/nightwatch -c ./nightwatch-cucumber-parallel.conf.js",
    "nightwatch-cucumber-firefox": "node_modules/.bin/nightwatch -c ./nightwatch-cucumber.conf.js -e firefox",
    "nightwatch-cucumber-ios": "node_modules/.bin/nightwatch -c ./nightwatch-cucumber-ios.conf.js -e default",
    "nightwatch-cucumber-chrome-firefox": "node_modules/.bin/nightwatch -c ./nightwatch-cucumber.conf.js -e chrome,firefox",
    "generate-report": "node generate-report.js"
  },
  "dependencies": {
    "apickli": "2.1.1",
    "chromedriver": "2.41.0",
    "cucumber": "4.2.1",
    "cucumber-html-reporter": "4.0.2",
    "cucumber-pretty": "1.4.1",
    "geckodriver": "1.12.1",
    "nightwatch": "0.9.20",
    "nightwatch-cucumber": "9.1.2",
    "selenium-server": "3.14.0"
  }
}
